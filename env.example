# ENV
NODE_ENV = 'Development'

# PORT
PORT = 3000

# LOG
LOG_LEVEL = silly

# DATABASE
DB_HOST = 'hosting/localhost'
DB_USER = 'database user'
DB_PASS = 'database password
DB_DATABASE = 'database name'

# AUTH
AUTH_SECRET = Complete with a password to encrypt the token (JWT)
AUTH_TIME = Duration of the token, can it be either 1h, 1d, 1w.

# AWS
AWS_PUBLIC_ACCESS_KEY = Public aws access key
AWS_PRIVATE_ACCESS_KEY = Private aws access key
AWS_S3_BUCKET_NAME = Name of the bucket made in AWS